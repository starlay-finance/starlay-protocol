{
  "name": "@starlay-finance/starlay-protocol",
  "version": "0.0.1",
  "description": "Starlay Protocol smart contracts",
  "files": [
    "contracts",
    "artifacts"
  ],
  "scripts": {
    "run-env": "npm i && tail -f /dev/null",
    "hardhat": "hardhat",
    "hardhat:tenderly-main": "hardhat --network tenderlyMain",
    "hardhat:docker": "hardhat --network hardhatevm_docker",
    "hardhat:shibuya": "hardhat --network shibuya",
    "compile": "SKIP_LOAD=true hardhat compile",
    "console:fork": "FORK=main hardhat console",
    "test": "npm run compile && TS_NODE_TRANSPILE_ONLY=1 hardhat test ./test-suites/test-starlay/*.spec.ts",
    "test-scenarios": "npm run compile && hardhat test test-suites/test-starlay/__setup.spec.ts test-suites/test-starlay/scenario.spec.ts",
    "test-subgraph:scenarios": "npm run compile && hardhat test test-suites/test-starlay/__setup.spec.ts test-suites/test-starlay/subgraph-scenarios.spec.ts",
    "test:main:check-list": "npm run compile && FORK=main TS_NODE_TRANSPILE_ONLY=1 hardhat test  test-suites/test-starlay/__setup.spec.ts test-suites/test-starlay/astar/check-list.spec.ts",
    "dev:coverage": "buidler compile --force && buidler coverage --network coverage",
    "starlay:dev:migration": "npm run compile && hardhat starlay:dev",
    "starlay:docker:full:migration": "npm run compile && npm run hardhat:docker -- starlay:astar --skip-registry",
    "starlay:docker:full:migration:add-registry": "npm run compile && npm run hardhat:docker -- starlay:astar",
    "starlay:docker:add-market-to-registry-from-config": "npm run compile && npm run hardhat:docker -- add-market-to-registry --pool Starlay",
    "starlay:shibuya:add-market-to-registry-from-config": "npm run compile && npm run hardhat:shibuya -- add-market-to-registry --pool Starlay",
    "starlay:docker:add-market-to-new-registry": "npm run compile && npm run hardhat:docker -- add-market-to-registry --pool Starlay --deploy-registry",
    "starlay:shibuya:add-market-to-new-registry": "npm run compile && npm run hardhat:shibuya -- add-market-to-registry --pool Starlay --verify --deploy-registry",
    "starlay:fork:main": "npm run compile && FORK=main hardhat starlay:astar",
    "starlay:shibuya:full:migration": "npm run compile && npm run hardhat:shibuya -- starlay:astar",
    "starlay:main:full:initialize": "npm run compile && FORK=main full:initialize-tokens --pool Starlay",
    "prettier:check": "npx prettier -c 'tasks/**/*.ts' 'contracts/**/*.sol' 'helpers/**/*.ts'  'test-suites/test-starlay/**/*.ts'",
    "prettier:write": "prettier --write 'tasks/**/*.ts' 'contracts/**/*.sol' 'helpers/**/*.ts'  'test-suites/test-starlay/**/*.ts'",
    "ci:test": "npm run compile && npm run test",
    "ci:clean": "rm -rf ./artifacts ./cache ./types",
    "shibuya:verify": "npm run hardhat:shibuya verify:general -- --all --pool Starlay",
    "shibuya:verify:tokens": "npm run hardhat:shibuya verify:tokens -- --pool Starlay",
    "print-contracts:shibuya": "npm run hardhat:shibuya -- print-contracts",
    "print-config:shibuya": "hardhat --network shibuya print-config --pool Starlay --data-provider 0xA1901785c29cBd48bfA74e46b67C736b26054fa4",
    "shibuya:deployUIProviderV2": "hardhat --network shibuya deploy-UiPoolDataProviderV2",
    "shibuya:deployUIProviderV2V3": "hardhat --network shibuya deploy-UiPoolDataProviderV2V3",
    "shibuya:deployUIIncentivesProviderV2": "hardhat --network shibuya deploy-UiIncentiveDataProviderV2",
    "astar:deployUniswapRepayAdapter": "hardhat --network astar deploy-UniswapRepayAdapter --provider 0xB53C1a33016B2DC2fF3653530bfF1848a515c8c5 --router 0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D --weth 0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2",
    "astar:UniswapLiquiditySwapAdapter": "hardhat --network astar deploy-UniswapLiquiditySwapAdapter  --provider 0xB53C1a33016B2DC2fF3653530bfF1848a515c8c5 --router 0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D --weth 0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2",
    "astar:ParaSwapLiquiditySwapAdapter": "hardhat --network astar deploy-ParaSwapLiquiditySwapAdapter --provider 0xB53C1a33016B2DC2fF3653530bfF1848a515c8c5 --augustusRegistry 0xa68bEA62Dc4034A689AA0F58A76681433caCa663",
    "external:deploy-assets-shibuya": "npm run compile && hardhat --network shibuya external:deploy-new-asset --symbol ${SYMBOL} --verify",
    "prepublishOnly": "npm run compile"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.7",
    "@nomiclabs/buidler-ethers": "2.0.0",
    "@nomiclabs/buidler-etherscan": "^2.1.0",
    "@nomiclabs/buidler-waffle": "2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@openzeppelin/contracts": "3.1.0",
    "@tenderly/hardhat-tenderly": "1.1.0-beta.5",
    "@typechain/ethers-v4": "1.0.0",
    "@typechain/ethers-v5": "^2.0.0",
    "@typechain/truffle-v4": "2.0.2",
    "@typechain/truffle-v5": "2.0.2",
    "@typechain/web3-v1": "1.0.0",
    "@types/chai": "4.2.11",
    "@types/lowdb": "1.0.9",
    "@types/mocha": "7.0.2",
    "@types/node": "14.0.5",
    "bignumber.js": "9.0.0",
    "buidler-typechain": "0.2.2",
    "chai": "4.3.6",
    "chai-bignumber": "3.0.0",
    "chai-bn": "^0.2.1",
    "defender-relay-client": "^1.7.0",
    "dotenv": "^8.2.0",
    "eth-sig-util": "3.0.1",
    "ethereum-waffle": "3.4.0",
    "ethereumjs-util": "7.1.4",
    "ethers": "^5.0.19",
    "hardhat": "^2.2.0",
    "hardhat-gas-reporter": "^1.0.0",
    "hardhat-typechain": "^0.3.3",
    "husky": "^4.2.5",
    "lowdb": "1.0.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "solidity-coverage": "^0.7.19",
    "temp-hardhat-etherscan": "^2.0.2",
    "ts-generator": "^0.1.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typechain": "^4.0.0",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern 'contracts/**/*.sol' --pattern 'helpers/**/*.ts'  --pattern 'test-suites/test-starlay/**/*.ts' --pattern 'tasks/**/*.ts'"
    }
  },
  "author": "Starlay",
  "license": "AGPLv3",
  "dependencies": {
    "tmp-promise": "^3.0.2"
  },
  "keywords": [
    "starlay",
    "polkadot",
    "astar"
  ]
}
